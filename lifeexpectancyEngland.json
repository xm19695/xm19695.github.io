{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",

    "data": {
       "url": "lifeexpectancyEngland.csv"
       },


    "title": {
        "text": "Life Expectancy in England",
        "subtitle": "Source: ONS.GOV.UK",
        "subtitleFontStyle":"italic",
        "subtitleFontSize":10,
        "anchor": "start",
        "color": "black"},

    "width":300,
    "height":300,

    "mark":{
        "type": "line",
        "color":"orange",
        "strokeWidth":2,
        "opacity":1},
 

    "encoding": {
      "x": {
          "field": "time_period",
          "type": "temporal",
          "title":null},
      "y": {
          "field": "Life_Expectancy",
          "type": "quantitative",
          "title":null},
          "scale":{"domain": [70,85]},
      "color": {"field": "location", "type": "nominal", "title": "Country"}
    }
}